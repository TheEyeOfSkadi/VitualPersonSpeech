<Window x:Class="VitualPersonSpeech.VirtualHumanBg"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:VitualPersonSpeech"
        mc:Ignorable="d"
        Title="VirtualHumanBg" Height="450" Width="800" WindowStyle="None" WindowState="Maximized">
    <Grid>
        <Image x:Name="contentImage" Source="{Binding MyImage}" Margin="0,0,0,0" Stretch="Fill"/>
    </Grid>
</Window>
